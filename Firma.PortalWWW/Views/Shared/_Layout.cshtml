            <a id="logo-container" href="#" class="brand-logo">Logo</a>
            <ul class="right hide-on-med-and-down">
                @*Tu są wszystkie linki*@
                <li>
                    @Html.ActionLink("Home", "Index", "Home") @*nazwa linku - nazwa pliku - nazwa foldera*@
                </li>
                <li>
                    @Html.ActionLink("O Firmie", "About", "Home") @*nazwa linku - nazwa pliku - nazwa foldera*@
                </li>
                <li>
                    @Html.ActionLink("Polityka prywatności", "Privacy", "Home") @*nazwa linku - nazwa pliku - nazwa foldera*@
                </li>
                <li>
                    @Html.ActionLink("Kontakt", "Contact", "Home") @*nazwa linku - nazwa pliku - nazwa foldera*@
                </li>
            </ul>

            <ul id="nav-mobile" class="side-nav">
