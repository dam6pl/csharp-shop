                <li>
                    @Html.ActionLink("Kontakt", "Contact", "Home") @*nazwa linku - nazwa pliku - nazwa foldera*@
                </li>
            </ul>
            <a href="#" data-activates="nav-mobile" class="button-collapse"><i class="material-icons">menu</i></a>
        </div>
    </nav>
    <div class="section no-pad-bot" id="index-banner">
        <div class="container">
            <div class="row center"> @*centrowanie body*@ 
                @RenderBody()
            </div>
        </div>
    </div>


    <div class="container">
        <div class="section">

            <!--   Icon Section   -->
            <div class="row">
                <div class="col s12 m4">
                    <div class="icon-block">
