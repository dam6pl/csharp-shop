@model IEnumerable<Firma.Data.CMS.Aktualnosc>

<div class="section">
    <!--   Icon Section   -->
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="col s12 m4">
                <div class="icon-block">
                    <h2 class="center light-blue-text"><i class="material-icons">adb</i></h2>
                    <h5 class="center">@Html.ActionLink(item.LinkTytul, "index", "Aktualnosc", new { id = item.IdAktualnosc }, null)</h5>

                    <p class="light">@item.Tytul</p>
                </div>
            </div>
        }
    </div>
</div>